<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >
<suite name="Run test" parallel="tests" thread-count="3">

    <test name="Test with parameter from xml to setUp">
        <parameter name="PLAN" value="FREE"/>
        <classes>
            <class name="factoryTestNG.ParametersFromXmlToSetupTest"/>
        </classes>
    </test>

    <test name="Test with parameter from xml to setUp2">
        <parameter name="PLAN" value="PLUS"/>
        <classes>
            <class name="factoryTestNG.ParametersFromXmlToSetupTest"/>
        </classes>
    </test>

    <test name="Test with parameter from xml to setUp3">
        <parameter name="PLAN" value="PREMIUM"/>
        <classes>
            <class name="factoryTestNG.ParametersFromXmlToSetupTest"/>
        </classes>
    </test>

</suite>